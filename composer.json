{
  "name": "iroge/laravel-tron-module",
  "description": "Laravel Tron Module",
  "homepage": "https://github.com/iroge/laravel-tron-module",
  "license": "MIT",
  "authors": [
    {
      "name": "iRoge",
      "email": "iroge27@gmail.com"
    }
  ],
  "keywords": [
    "iRoge",
    "laravel",
    "tron"
  ],
  "require": {
    "php": ">=8.1",
    "ext-gmp": "*",
    "ext-ctype": "*",
    "illuminate/contracts": "^10.0|^11.0|^12.0",
    "spatie/laravel-package-tools": "^1.15",
    "furqansiddiqui/bip39-mnemonic-php": "^0.2.1",
    "minter/minter-php-bip-44": "^1.2",
    "web3p/web3.php": "^0.1.6",
    "kornrunner/secp256k1": "^0.3",
    "simplito/elliptic-php": "^1.0",
    "brick/math": ">=0.12"
  },
  "autoload": {
    "psr-4": {
      "Iroge\\LaravelTronModule\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Iroge\\LaravelTronModule\\TronServiceProvider"
      ],
      "aliases": {
        "Tron": "Iroge\\LaravelTronModule\\Facades\\Tron"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
